# syntax=docker/dockerfile:1
FROM node:14

# RUN apt-get install cron

# 작업 폴더 설정
WORKDIR /home
RUN git clone https://github.com/osamhack2022/WEB_CLOUD_OmilZomil_NAVYeffect
WORKDIR /home/WEB_CLOUD_OmilZomil_NAVYeffect/frontend/omilzomil
# 개발용 branch 로 이동
RUN git checkout -t origin/dev/rebase

ENTRYPOINT ["sh", "/home/WEB_CLOUD_OmilZomil_NAVYeffect/frontend/omilzomil/entrypoint.sh"]
